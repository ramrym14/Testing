    {
      "expr": "sum(container_memory_usage_bytes{image!=\"\", container!=\"\"}) / 1024 / 1024",
      "refId": "A"
    }
  ],
  "options": {
    "reduceOptions": {
      "calcs": ["lastNotNull"],
      "fields": "",
      "values": false
    },
    "textMode": "value",
