{
  "uid": "bdd-tests",
  "title": "BDD Test Dashboard",
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "stat",
      "title": "‚úÖ Tests Passed",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 3 },
      "targets": [
        {
          "expr": "tests_passed",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "‚ùå Tests Failed",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 3 },
      "targets": [
        {
          "expr": "tests_failed",
          "refId": "B"
        }
      ]
    },
    {
      "type": "stat",
      "title": "‚è± Test Duration (s)",
      "gridPos": { "x": 0, "y": 3, "w": 6, "h": 3 },
      "targets": [
        {
          "expr": "tests_duration_seconds",
          "refId": "C"
        }
      ]
    },
   {
